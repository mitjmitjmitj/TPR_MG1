public class Persoon {

  // VERVANG OVERAL DE PUNTJES DOOR HET JUISTE DATATYPE

  private String achternaam = "Meulendijk";
  private ... voornaam = "Loes";
  private ... nationaliteit = "Nederlandse";
  private int geboortejaar = 1971;
  private ... lengte = 1.71;
  private ... geslacht = 'V';
  private ... persoonsnummer = 123456782;
  private ... idKaartNogGeldig = false;

  public ... getAchternaam() {
    return achternaam;
  }

  public String getVoornaam() {
    return voornaam;
  }

  public ... getNationaliteit() {
    return nationaliteit;
  }

  public ... getGeboortejaar() {
    return geboortejaar;
  }

  public double getLengte() {
    return lengte;
  }

  public char getGeslacht() {
    return geslacht;
  }

  public int getPersoonsnummer() {
    return persoonsnummer;
  }

  public ... getIdKaartNogGeldig() {
    return idKaartNogGeldig;
  }

  public static ... main(...[] args) {
    ... loes = new Persoon();
    loes.showSomeIDPlease();
  }

  public void showSomeIDPlease() {
    System.out.println("\nAchternaam:\t\t" + getAchternaam());
    System.out.println("Voornaam:\t\t" + getVoornaam());
    System.out.println("Nationaliteit:\t\t" + getNationaliteit());
    System.out.println("Geboortejaar:\t\t" + getGeboortejaar());
    System.out.println("Lengte:\t\t\t" + getLengte());
    System.out.println("Geslacht:\t\t" + getGeslacht());
    System.out.println("Persoonsnummer:\t\t" + getPersoonsnummer());
    System.out.println("ID-kaart nog geldig:\t" + getIdKaartNogGeldig() + "\n");
  }

}
